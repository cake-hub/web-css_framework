{
  "name": "@cake-hub/web-css_framework",
  "versions": {
    "Cake": "Apple Pie"
  },
  "version": "4.2.0",
  "description": "CSS Framework for the Schwarz Group",
  "repository": {
    "type": "git",
    "url": "https://github.com/cake-hub/web-css_framework"
  },
  "homepage": "https://cake.schwarz",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "style": "dist/Cake/css/cake.css",
  "sass": "themes/Cake/scss/cake.scss",
  "main": "themes/Cake/js/cake.js",
  "_moduleAliases": {
    "@Cake": "./",
    "@Develop": "./docs",
    "@Site": "./site",
    "@Toolkit": "scss",
    "@node_modules": "node_modules",
    "@root": "./"
  },
  "scripts": {
    "build": "npm run delete-generated && npm run build-assets && npm run build-examples",
    "release": "npm run build && npm run lint-all",
    "build-assets": "webpack --progress",
    "build-examples": "babel-node ./build/exampleIndex.js",
    "delete-generated": "rm -Rf ./dist ./docs/_assets ./docs/**/examples",
    "dev": "NODE_ENV=develop npm run build-assets && npm-run-all --parallel watch-assets watch-examples",
    "dev-server": "npm-run-all --parallel dev server",
    "watch-assets": "webpack --watch",
    "watch-examples": "nodemon --watch \"docs/**/*.html.js\" --watch \"docs/**/page.js\" --exec npm run build-examples",
    "lint-all": "npm run lint-scss && npm run lint-md",
    "lint-scss": "stylelint ./scss",
    "lint-md": "markdownlint ./docs/**/*md",
    "server": "http-server ./docs -p 2021"
  },
  "dependencies": {
    "hamburgers": "^1.1.3",
    "svg4everybody": "^2.1.9",
    "tiny-slider": "^2.9.1",
    "tippy.js": "^6.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.5",
    "core-js": "^3.6.5",
    "css-loader": "^2.1.0",
    "express": "^4.16.4",
    "fibers": "^5.0.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "filter-css": "^0.1.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.3",
    "html-minifier": "^4.0.0",
    "http-server": "^0.12.3",
    "iframe-resizer": "^4.1.1",
    "js-beautify": "^1.9.0",
    "js-yaml": "^3.13.1",
    "markdown-to-jsx": "^6.10.1",
    "markdownlint": "^0.17.2",
    "markdownlint-cli": "^0.19.0",
    "mini-css-extract-plugin": "^0.5.0",
    "module-alias": "^2.1.0",
    "node-html-parser": "^1.1.19",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-inline-svg": "^3.1.1",
    "postcss-loader": "^3.0.0",
    "purify-css": "^1.2.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "regenerator-runtime": "^0.13.5",
    "sass": "^1.26.8",
    "sass-loader": "^7.2.0",
    "shortid": "^2.2.14",
    "strip-css-comments": "^4.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-twbs-bootstrap": "0.3.0",
    "svg-spritemap-webpack-plugin": "^3.2.1",
    "uglify-js": "^3.7.5",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-fix-style-only-entries": "^0.2.0"
  },
  "author": "CEA",
  "license": "Ms-RSL"
}
